{
  "name": "mancho-school-web",
  "version": "1.0.0",
  "author": "Esen Sagynov <esen@mancho.dev>",
  "description": "Mancho School base website",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "build:beta": "GATSBY_SITE_DOMAIN=www.beta.mancho.school gatsby build",
    "clean": "npm run clean:soft && rm -rf .cache public node_modules package-lock.json",
    "clean:soft": "gatsby clean && npm run clean:test",
    "start": "gatsby serve",
    "clean:test": "jest --clearCache",
    "deploy": "gatsby-plugin-s3 deploy -y",
    "dev": "gatsby develop --host=0.0.0.0",
    "prepare-env": "node ./prepare-env.js",
    "test": "npm run clean:test && jest src/** --coverage"
  },
  "engines": {
    "node": "~18",
    "npm": "~9"
  },
